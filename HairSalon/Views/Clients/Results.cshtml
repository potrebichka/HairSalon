@{
    Layout = "_Layout";
}

<h1>Found Client:</h1>

@if (Model == null)
{
    <h3>No clients have been found.</h3>
}
else
{
    <h3><li>@Html.ActionLink($"{Model.Name}", "Details", new {id = Model.ClientId} )</li></h3>
}

<h4>@Html.ActionLink("Search", "Search")</h4>
<h4>@Html.ActionLink("Home", "Index", "Home")</h4>